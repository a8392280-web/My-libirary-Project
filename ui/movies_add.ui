    padding: 4px;              /* Space inside */
}

QWidget { 
	background-color: #2b3640;
}

QPushButton {
    color: #e0e6f0; /* light text */
    font-size: 14px;
    background-color: #2e3a4b; /* dark gray-blue button */
    border: 1px solid rgba(255, 255, 255, 0.15);
    border-radius: 6px;
    padding: 6px 14px;
}

QPushButton:hover {
    background-color: #3c4d63; /* lighter on hover */
    border: 1px solid #5891ff; /* subtle blue glow */
}

QPushButton:pressed {
    background-color: #1e2a3a; /* darker when pressed */
    border: 1px solid #4f7de0;
}

QPushButton:disabled {
    color: rgba(255, 255, 255, 0.4);
    background-color: rgba(255, 255, 255, 0.05);
    border: 1px solid rgba(255, 255, 255, 0.1);
}

QComboBox {
    color: #e0e6f0; /* light text */
