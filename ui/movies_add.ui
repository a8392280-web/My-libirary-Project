    background-color: rgba(255, 255, 255, 0.05); /* subtle transparent bg */
    border: 1px solid rgba(255, 255, 255, 0.15);
    border-radius: 6px;
    padding: 6px 10px;
}

QComboBox:hover {
    background-color: rgba(255, 255, 255, 0.1);
    border: 1px solid #5891ff; /* subtle blue border */
}

QComboBox:focus {
    border: 1px solid #5891ff;
    background-color: rgba(255, 255, 255, 0.12);
    outline: none;
}

/* â–¼ Dropdown arrow */
QComboBox::drop-down {
    border: none;
    width: 25px;
    background-color: transparent;
}

QComboBox::down-arrow {
    image: url(:/icons/Icons/sort_by.png); /* replace with your icon */
